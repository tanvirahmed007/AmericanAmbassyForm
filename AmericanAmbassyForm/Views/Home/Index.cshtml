@using Microsoft.AspNetCore.Identity
@using AmericanAmbassyForm.Areas.Identity.Data
@model FormInformation

@inject SignInManager<AmericanAmbassyFormUser> SignInManager
@inject UserManager<AmericanAmbassyFormUser> UserManager

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">

    @if (SignInManager.IsSignedIn(User))
{
   
        <h1>Hello @UserManager.GetUserName(User)!</h1>
    
    
}
    <h1 class="display-4">Welcome  American Ambassy Form Registration</h1>

    <form action="" method="post">
        <input type="text" asp-for="Name" placeholder="Given Name" />
        <input type="text" asp-for="Surname" placeholder="Surname" />
        <input type="date" asp-for="DOB" />
        
        <input type="text" asp-for="Age" />
        <input type="text" asp-for="Email" placeholder="Email" />
        <input type="text" asp-for="Phone" placeholder="Contact Number" />
        
        
        <input type="submit" value="Submit" />
    </form>
    
</div>
